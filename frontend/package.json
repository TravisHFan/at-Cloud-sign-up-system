{
  "name": "frontend",
  "private": true,
  "version": "2.4.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && cp dist/index.html dist/404.html && cp static.json dist/static.json && cp public/_redirects dist/_redirects",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "verify": "npm run lint && npm run type-check",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 vitest run",
    "test:watch": "NODE_OPTIONS=--max-old-space-size=4096 vitest --watch",
    "test:verbose": "NODE_OPTIONS=--max-old-space-size=4096 vitest run --reporter=verbose",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "type-check": "tsc -b --pretty false"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-router-dom": "^7.6.3",
    "socket.io-client": "^4.8.1",
    "xlsx": "^0.18.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "axe-core": "^4.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/xlsx": "^0.0.35",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vitest": "^1.0.0"
  }
}
